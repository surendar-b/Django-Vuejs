{"ast":null,"code":"import TreeBody from './TreeBody';\nimport TreeTableHeader from './TreeTableHeader';\nimport { uuid } from 'vue-uuid';\nexport default {\n  name: 'TreeTable',\n  components: {\n    TreeBody,\n    TreeTableHeader\n  },\n  props: {\n    tableData: {\n      type: Array,\n      default: () => {\n        return [];\n      }\n    },\n    columns: {\n      type: Array,\n      default: () => {\n        return [];\n      }\n    }\n  },\n  computed: {\n    columnsOrder() {\n      return this.columns.map(column => column.id);\n    }\n\n  },\n\n  created() {\n    this.enrichTableData();\n  },\n\n  methods: {\n    addUniqueId(data) {\n      data.uuid = uuid.v4();\n\n      if (data.children) {\n        data.children.forEach(child => this.addUniqueId(child));\n      }\n    },\n\n    enrichTableData() {\n      this.tableData.forEach(el => this.addUniqueId(el));\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgCE,OAAOA,QAAP,MAAqB,YAArB;AACA,OAAOC,eAAP,MAA4B,mBAA5B;AACA,SAASC,IAAT,QAAqB,UAArB;AAEA,eAAe;EACbC,IAAI,EAAE,WADO;EAEbC,UAAU,EAAE;IAACJ,QAAD;IAAWC;EAAX,CAFC;EAGbI,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAEC,KADG;MAETC,OAAO,EAAE,MAAM;QAAE,OAAO,EAAP;MAAU;IAFlB,CADN;IAKLC,OAAO,EAAE;MACPH,IAAI,EAAEC,KADC;MAEPC,OAAO,EAAE,MAAM;QAAE,OAAO,EAAP;MAAU;IAFpB;EALJ,CAHM;EAabE,QAAQ,EAAE;IACRC,YAAY,GAAE;MACZ,OAAO,KAAKF,OAAL,CAAaG,GAAb,CAAiBC,MAAK,IAAKA,MAAM,CAACC,EAAlC,CAAP;IACD;;EAHO,CAbG;;EAkBbC,OAAO,GAAG;IACN,KAAKC,eAAL;EACH,CApBY;;EAqBbC,OAAO,EAAE;IACPC,WAAW,CAACC,IAAD,EAAM;MACfA,IAAI,CAAClB,IAAL,GAAYA,IAAI,CAACmB,EAAL,EAAZ;;MACA,IAAID,IAAI,CAACE,QAAT,EAAmB;QACfF,IAAI,CAACE,QAAL,CAAcC,OAAd,CAAsBC,KAAI,IAAK,KAAKL,WAAL,CAAiBK,KAAjB,CAA/B;MACJ;IACD,CANM;;IAOPP,eAAe,GAAG;MACd,KAAKX,SAAL,CAAeiB,OAAf,CAAuBE,EAAC,IAAK,KAAKN,WAAL,CAAiBM,EAAjB,CAA7B;IACJ;;EATO;AArBI,CAAf","names":["TreeBody","TreeTableHeader","uuid","name","components","props","tableData","type","Array","default","columns","computed","columnsOrder","map","column","id","created","enrichTableData","methods","a